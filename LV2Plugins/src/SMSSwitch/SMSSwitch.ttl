@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .

<https://github.com/sirsipe/SudoMetalStudio#me>
    a foaf:Person ;
    foaf:name "SudoMetalStudio" ;
    foaf:homepage "https://github.com/sirsipe/SudoMetalStudio" .

<https://github.com/sirsipe/SudoMetalStudio/LV2Plugins/SMSSwitch> a lv2:MixerPlugin ;
    lv2:project <https://github.com/sirsipe/SudoMetalStudio> ;
    doap:name "SMSSwitch" ;
    lv2:optionalFeature lv2:hardRTCapable ;
    lv2:minorVersion 1 ;
    lv2:microVersion 0 ;
    doap:maintainer <https://github.com/sirsipe/SudoMetalStudio#me> ;

    lv2:port 
        [
            a lv2:InputPort , lv2:AudioPort ;
            lv2:index 0 ;
            lv2:symbol "audio_in" ;
            lv2:name "Audio input" ;
        ] ,
        [
            a lv2:OutputPort , lv2:AudioPort ;
            lv2:index 1 ;
            lv2:symbol "audio_out" ;
            lv2:name "Audio output" ;
        ] ,

        [
            a lv2:InputPort , lv2:ControlPort ;
            lv2:index 2 ;
            lv2:symbol "enable" ;
            lv2:name "Enable" ;
            lv2:default 1.0 ;
            lv2:minimum 0.0 ;
            lv2:maximum 1.0 ;
            lv2:designation lv2:enabled;
            lv2:portProperty lv2:toggled;
        ] .